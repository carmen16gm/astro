---
import { getCollection } from 'astro:content';

const entries = await getCollection('dynamic');
---

<!DOCTYPE html>
<html lang="en">
  <head><meta charset="utf-8" /></head>
	<body>
		{entries.map(async entry => {
			const { Content } = await entry.render();
			return <Content mdProps="work" />; 
		})}
	</body>
</html>
